{
	"compilerOptions": {
		"noImplicitAny": false,
		"noImplicitReturns": true,
		"noUnusedLocals": false,
		"noUnusedParameters": true,
		"allowSyntheticDefaultImports": true,
		"lib": [
			"es6"
		],
		"module": "commonjs",
		"moduleResolution": "node",
		"target": "es6",
		"declaration": true,
		"sourceMap": true
	},
	"exclude": [
		"node_modules",
		"dist"
	]
}