{
	"name": "beat language syntax highlights",
	"version": "0.3.0",
	"scopeName": "source.beat",
	"fileTypes": [
	  "b"
	],
	"patterns": [
	  {
		"match": "\\/.*\\/",
		"name": "comment"
	  },
	  {
		"match": "[\\[|\\]|\\{|\\}|\\(|\\)]",
		"name": "entity.name.tag"
	  },
	  {
		"match": "\\=",
		"name": "support.function"
	  },
	  {
		"match": "\\?",
		"name": "support.function"
	  },
	  {
		"match": "\\.\\.\\.",
		"name": "support.function"
	  },
	  {
		"match": "\\:\\:",
		"name": "support.function"
	  },
	  {
		"match": "l",
		"name": "support.function"
	  },
	  {
		"match": "->",
		"name": "support.function"
	  },
	  {
		"match": "=[>|=]",
		"name": "support.function"
	  },
	  {
		"match": "string",
		"name": "support.constant"
	  },
	  {
		"match": "number",
		"name": "support.constant"
	  },
	  {
		"match": "bool",
		"name": "support.constant"
	  },
	  {
		"match": "\\#[A-Za-z0-9]+",
		"name": "string"
	  },
	  {
		"match": "\\@[A-Za-z0-9]+\\:",
		"name": "support.function"
	  },
	  {
		"match": "[A-Za-z0-9]+\\:",
		"name": "entity.name"
	  },
	  {
		"match": "[A-Za-z0-9]+\\:",
		"name": "keyword.control"
	  },
	  {
		"match": "[A-Za-z0-9]+",
		"name": "string"
	  }
	]
  }